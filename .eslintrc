{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
    },
    "env": {
        "node": true,
        "es6": true,
        "browser": true,
        "jquery": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "error",
            4, {
                "SwitchCase": 1
            }
        ],
        "linebreak-style": [
            "error", "unix"
        ],
        "quotes": [
            "error", "single", { "allowTemplateLiterals": true }
        ],
        "semi": [
            "error", "always"
        ],
        "no-console": 0,
    }
}
